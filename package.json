{
  "name": "deploy-appengine",
  "version": "0.5.0",
  "description": "Github Action: Deploy to Google App Engine",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build -m src/index.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lintfix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write **.ts",
    "test": "mocha -r ts-node/register -t 150s 'tests/*.test.ts' --exit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google-github-actions/deploy-appengine"
  },
  "keywords": [
    "actions",
    "google cloud",
    "app engine",
    "deploy",
    "app"
  ],
  "author": "Google LLC",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.0",
    "@google-github-actions/setup-cloud-sdk": "^0.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.x",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.12",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vercel/ncc": "^0.33.0",
    "chai": "^4.3.x",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.x",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "sinon": "^12.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
